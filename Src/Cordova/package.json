{
  "name": "com.dwsg.pocketinvestigator",
  "displayName": "Pocket Investigator",
  "version": "0.0.999",
  "description": "An interactive fiction game about being an investigator.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Dick Wolff",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-ios": "^6.1.0",
    "cordova-plugin-admob-free": "^0.27.0",
    "cordova-plugin-play-games-services": "^1.1.2",
    "cordova-plugin-purchase": "^10.2.0",
    "cordova-plugin-splashscreen": "^5.0.4",
    "cordova-promise-polyfill": "0.0.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-admob-free": {
        "ADMOB_APP_ID": "{{ANDROID_ADMOB_ID}}"
      },
      "cordova-plugin-play-games-services": {
        "APP_ID": "{{ANDROID_PLAY_SERVICES_ID}}"
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-purchase": {
        "BILLING_KEY": "{{ANDROID_BILLING_KEY}}"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  }
}
